{
  "expo": {
    "name": "attendance_checking",
    "slug": "attendance_checking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash_icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash_screen_bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash_screen_bg.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "/assets/images/favicon.pngy"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": ["expo-router"],
    "scheme": "attendance-checker",
    "runtimeVersion": "1.0.0"
  }
}
